{
  "name": "mapi",
  "version": "1.0.5",
  "description": "An easy to use wrapper for Google Maps API",
  "main": "mapi.js",
  "scripts": {
    "build-normal": "webpack src/mapi.js mapi.js",
    "build-min": "webpack -p src/mapi.js mapi.min.js",
    "build": "npm run build-normal && npm run build-min",
    "dev": "webpack -w src/mapi.js mapi.js"
  },
  "repository": {
